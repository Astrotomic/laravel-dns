{
    "name": "astrotomic/laravel-dns",
    "type": "library",
    "description": "",
    "keywords": [
        "astrotomic",
        "laravel-dns",
        "laravel",
        "dns"
    ],
    "homepage": "https://astrotomic.info",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Witkowski",
            "email": "gummibeer@astrotomic.info",
            "homepage": "https://gummibeer.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/collections": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/translation": "^8.0",
        "spatie/dns": "^2.0.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.19",
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Astrotomic\\Dns\\DnsServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Astrotomic\\Dns\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "fix": "vendor/bin/php-cs-fixer fix --using-cache=no --config=.php_cs.dist",
        "test": "vendor/bin/pest",
        "test-coverage": "XDEBUG_MODE=coverage vendor/bin/pest --coverage"
    }
}
